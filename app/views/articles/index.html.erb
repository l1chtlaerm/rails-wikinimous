<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <% @articles.each do |article| %>
         <article>
            <h1><%= link_to article.title, article_path(article) %></h1>
            <%= article.content %>
         </article>
      <% end %>
    </div>
  </div>
</div>
